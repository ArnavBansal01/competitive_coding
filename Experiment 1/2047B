#include <bits/stdc++.h>
using namespace std;

long long factorial(int x) {
    long long res = 1;
    for (int i = 1; i <= x; i++) {
        res *= i;
    }
    return res;
}

int main() {
    int t;
    cin >> t;

    while (t--) {
        int n;
        string s;
        cin >> n >> s;

        long long minWays = -1;
        string answer = s;

        for (int a = 0; a < n; a++) {
            for (int b = 0; b < n; b++) {
                string cur = s;
                cur[a] = s[b];

                int freq[26];
                for (int i = 0; i < 26; i++) freq[i] = 0;

                for (int i = 0; i < n; i++) {
                    freq[cur[i] - 'a']++;
                }

                long long total = factorial(n);
                for (int i = 0; i < 26; i++) {
                    if (freq[i] > 1) {
                        total /= factorial(freq[i]);
                    }
                }

                if (minWays == -1 || total < minWays) {
                    minWays = total;
                    answer = cur;
                }
            }
        }

        cout << answer << "\n";
    }

    return 0;
}
  
